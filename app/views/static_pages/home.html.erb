<div class="center jumbotron">
  <h1>Welcome to the Task List</h1>

  <% unless user_signed_in? or admin_signed_in? %>
    <%= link_to "Sign up now!", '/users/sign_up', class: "btn btn-lg btn-primary" %>
  <% end %>
</div>

<section id="content">
    <div class="row">
      <div class="col-md-8">
        <img src="https://cdn.pixabay.com/photo/2016/03/31/19/50/checklist-1295319_960_720.png" alt="task" width="600">
      </div>

      <div class="col-md-4">
        <div class="block-heading-two">
          <h3><span>Наши решения</span></h3>
        </div>

